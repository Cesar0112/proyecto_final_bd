	<parameter name="id_team" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT phase.phase_name, total_games.cant as JJ, total_win_games.cant as JG, (total_games.cant-total_win_games.cant) as JP, local_win_games.cant as JGL, (local_games.cant - local_win_games.cant) as JPL, (total_win_games.cant-local_win_games.cant) as JGV, ((total_games.cant-local_games.cant)-(total_win_games.cant-local_win_games.cant)) as JPV
  FROM phase
  JOIN (SELECT game.phase_id, count(game.game_id) as cant
        FROM team
        JOIN game ON team.team_id = game.team_home_club OR team.team_id = game.team_visitant
        WHERE team.team_id = $P!{id_team}
        GROUP BY game.phase_id) as total_games ON phase.phase_id = total_games.phase_id
  JOIN (SELECT game.phase_id, count(game.game_id) as cant
        FROM team 
        JOIN game ON team.team_id = game.team_home_club OR team.team_id = game.team_visitant
        WHERE team.team_id =$P!{id_team} AND game.winner = $P!{id_team}
        GROUP BY game.phase_id) as total_win_games ON phase.phase_id = total_win_games.phase_id
  JOIN (SELECT game.phase_id, count(game.game_id) as cant
        FROM team
        JOIN game ON team.team_id = game.team_home_club
        WHERE game.team_home_club = $P!{id_team} AND team.team_id = $P!{id_team}
        GROUP BY game.phase_id) as local_games ON phase.phase_id = local_games.phase_id
  JOIN (SELECT game.phase_id, count(game.game_id) as cant
        FROM team 
        JOIN game ON team.team_id = game.team_home_club
        WHERE game.team_home_club =$P!{id_team} AND game.winner = $P!{id_team}
        GROUP BY game.phase_id) as local_win_games ON phase.phase_id = local_win_games.phase_id;]]>
	</queryString>
